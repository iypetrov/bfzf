{
  "manifest_version": 2,
  "name": "bfzf",
  "version": "2.0.0",
  "description": "Browser extension that increase your productivity",
  "permissions": [
    "history",
    "tabs",
    "bookmarks"
  ],
  "commands": {
    "open_history": {
      "description": "Open history"
    },
    "open_bookmarks": {
      "description": "Open bookmarks"
    }
  },
  "background": {
    "scripts": [
      "dist/background/index.js"
    ]
  }
}