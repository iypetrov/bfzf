{
  "manifest_version": 2,
  "name": "bfzf",
  "version": "2.0.0",
  "description": "Browser extension that allows you to make your browsing experience more efficient.",
  "permissions": [
    "history",
    "tabs",
    "bookmarks"
  ],
  "icons": {
    "48": "assets/logo.png"
  },
  "commands": {
    "open_history": {
      "description": "Open history"
    },
    "open_bookmarks": {
      "description": "Open bookmarks"
    }
  },
  "background": {
    "scripts": [
      "dist/background/index.js"
    ]
  }
}