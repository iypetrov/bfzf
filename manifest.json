{
  "manifest_version": 2,
  "name": "bfzf",
  "version": "2.0.0",
  "description": "Browser extension that increase your productivity",
  "permissions": [
    "history",
    "tabs",
    "bookmarks"
  ],
  "icons": {
    "48": "public/logo192.png"
  },
  "commands": {
    "open_history": {
      "suggested_key": {
        "default": "Ctrl+Alt+H"
      },
      "description": "Open history"
    },
    "open_bookmarks": {
      "suggested_key": {
        "default": "Ctrl+Alt+B"
      },
      "description": "Open bookmarks"
    }
  },
  "background": {
    "scripts": [
      "dist/background/index.js"
    ]
  }
}